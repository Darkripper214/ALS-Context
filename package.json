    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.20",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "cls-hooked": "^4.2.2",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^8.1.0",
